<h1>Please Edit Agent Data</h1>
<%= form_with model: @user, local: true do |f| %>
    <%= f.label :email %>
    <br>
    <%= f.email_field :email %>
    <br>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <%= f.submit "Update User" %>

<% end %>