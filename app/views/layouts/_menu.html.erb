<% if is_admin? %>
    <%= link_to 'Add new property', new_property_path %>
    <%= link_to 'Add new agent', new_user_path %>
    <%= link_to 'Logged Out', admin_logged_out_path, method: :delete %>
<% elsif is_user? %>
    <%= link_to 'My Clients', clients_path %>
    <%= link_to 'Add client', new_client_path %>
    <%= link_to 'Logged Out', session_path, method: :delete %>
<% else %>
    <%= link_to 'Login', new_session_path %>
    <%= link_to 'Sign Up', new_user_path %>
<% end %>