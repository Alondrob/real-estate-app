<h1>Please add the property</h1><br>

<%= form_with model: @property, local: true do |f| %>
    <%= f.label :management %>
    <br>
    <%= f.text_field :management %>
    <br>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :size %>
    <%= f.text_field :size %>
    <%= f.label :neighborhood %>
    <%= f.password_field :neighborhood %>
    <%= f.label :neighborhood %>
    <%= f.password_field :neighborhood %>

    <%= f.submit "New property" %>

<% end %>